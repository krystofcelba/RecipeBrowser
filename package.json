{
  "name": "RecipeBrowser",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "watchman watch-del-all && node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-with-coverage": "jest --collectCoverage",
    "remotedev": "remotedev --hostname=localhost --port=8000"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "react": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-native-action-button": "^2.8.1",
    "react-native-elements": "^0.17.0",
    "react-native-i18n": "^2.0.9",
    "react-native-image-crop-picker": "^0.18.1",
    "react-native-navigation": "^1.1.253",
    "react-native-tableview-simple": "^0.17.1",
    "react-native-vector-icons": "^4.4.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/react": "^16.0.18",
    "@types/react-native": "^0.49.4",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "21.2.1",
    "prettier": "^1.7.4",
    "react-native-typescript-transformer": "^1.1.4",
    "react-test-renderer": "16.0.0-beta.5",
    "redux-saga-test-plan": "^3.3.0",
    "redux-testkit": "^1.0.6",
    "remote-redux-devtools": "^0.5.12",
    "remotedev-server": "^0.2.4",
    "ts-jest": "^21.1.4",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.5.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": false,
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
